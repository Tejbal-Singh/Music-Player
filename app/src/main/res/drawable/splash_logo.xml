<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <gradient android:startColor="#DD8D16"
                android:centerColor="#E6A33F"
                android:endColor="#E2B167"/>

            <stroke android:width="1dp"
                android:color="#191911"/>
        </shape>
    </item>
    <item
        android:top="6dp"
        android:left="6dp"
        android:bottom="6dp"
        android:right="6dp">
        <shape android:shape="rectangle">
            <size android:width="10dp"
                android:height="10dp"/>
            <corners android:bottomRightRadius="4dp"
                android:topLeftRadius="4dp"/>
            <stroke android:width="1dp"
                android:color="#0C0C0C"/>
            <gradient android:startColor="#FF9800"
                android:centerColor="#fff"
                android:angle="270"
                android:endColor="#0FE117"/>
        </shape>
    </item>
    <item
        android:top="5dp"
        android:right="5dp"
        android:left="5dp"
        android:bottom="5dp">
        <inset android:drawable="@drawable/baseline_music_note_24" />


    </item>
</layer-list>